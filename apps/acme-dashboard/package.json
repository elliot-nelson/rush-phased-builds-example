{
  "name": "@acme/dashboard",
  "description": "This project illustrates a minimal tutorial Heft project targeting the web browser runtime",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "build": "heft test --clean && node scripts/readme_updater.js",
    "start": "heft start",

    "_phase:build": "heft build --lite --clean",
    "_phase:lint": "eslint src",
    "_phase:test": "heft test --no-build",
    "_phase:readme_updater": "node scripts/readme_updater.js"
  },
  "devDependencies": {
    "@rushstack/eslint-config": "~2.4.1",
    "@rushstack/heft": "^0.39.2",
    "@rushstack/heft-jest-plugin": "~0.1.33",
    "@rushstack/heft-webpack4-plugin": "^0.3.0",
    "@types/heft-jest": "1.0.2",
    "@types/react": "16.9.45",
    "@types/react-dom": "16.9.8",
    "@types/webpack-env": "1.13.0",
    "css-loader": "~4.2.1",
    "eslint": "~7.32.0",
    "html-webpack-plugin": "~4.5.0",
    "react": "~16.13.1",
    "react-dom": "~16.13.1",
    "style-loader": "~1.2.1",
    "typescript": "~4.3.5",
    "webpack": "~4.44.2",
    "source-map-loader": "~1.1.2",
    "@acme/classic-rig": "workspace:*"
  },
  "dependencies": {
    "@acme/utils": "workspace:*"
  }
}
